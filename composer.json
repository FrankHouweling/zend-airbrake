{
    "name": "frank-houweling/zend-airbrake",
    "description": "Airbrake (phpbrake) integration for Zend Framework 3 (ZF3)",
    "type": "library",
    "license": "GPL-2.0",
    "keywords": [
        "airbrake",
        "phpbrake",
        "exception",
        "error",
        "logging",
        "zf3",
        "zend"
    ],
    "homepage": "http://www.senet.nl/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "airbrake/phpbrake": "^0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.17 || ^6.2.1"
    },
    "autoload": {
        "psr-4": {
            "FrankHouweling\\ZendAirbrake\\": "src/"
        }
    },
    "autoload-dev":      {
        "psr-4": {
            "FrankHouweling\\ZendAirbrakeTest\\": "tests/"
        }
    },
    "extra": {
        "zf": {
            "component": "FrankHouweling\\ZendAirbrake"
        }
    }
 }
